{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "./cadeosio/",
    "outDir": "./cadeosio/",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es6",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types",
      "node"
    ],
    "lib": [
      "es2018",
      "dom"
    ],

    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "allowJs": true
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
